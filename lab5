from turtle import Turtle
import random
class Square(Turtle):
    def __init__(self,size):
        Turtle.__init__(self)
        self.shapesize(size)
        self.shape("square")

    def Random_color(self):
        g = random.randint(0,256)
        r = random.randint(0,256)
        b = random.randint(0,256)
        self.color(r,g,b)
square1 = Square(10)
square1.Random_color()
